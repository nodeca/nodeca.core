nav.navbar-alt.navbar.navbar-dark.bg-primary.fixed-top.navbar-expand
  .navbar-inner.layout__container.container-fluid
    .navbar-nav
      a.navbar__level-up.nav-link.icon.icon-level-up(href=self.level_up)

    != self.partial('@common.blocks.navbar.blocks.page_progress', self.progress)

    .navbar-alt__title.navbar-text.d-none.d-sm-inline-block.mr-auto
      if self.title
        = self.title
      else if self.title_html
        != self.title_html

    if self.toolbar
      != self.partial('@' + self.toolbar)
