@charset "UTF-8";

@font-face {
  font-family: 'nodeca';
  src: url(<%= asset_path('fontello/nodeca.eot') %>);
  src: url(<%= asset_path('fontello/nodeca.eot') %>?#iefix) format('embedded-opentype'),
       url(<%= asset_path('fontello/nodeca.woff') %>) format('woff'),
       url(<%= asset_path('fontello/nodeca.ttf') %>) format('truetype'),
       url(<%= asset_path('fontello/nodeca.svg') %>#icons) format('svg');
  font-weight: normal; font-style: normal;
}


